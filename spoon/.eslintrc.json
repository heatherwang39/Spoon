{
    "extends": [
        "airbnb",
        "prettier"
        // "plugin:react/recommended",
        // "plugin:node/recommended"
    ],

    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off"
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
